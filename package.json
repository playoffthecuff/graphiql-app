{
  "name": "graphql-app",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "prepare": "husky",
    "test": "jest --passWithNoTests",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@apollo/client": "latest",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@fontsource/roboto": "latest",
    "@hookform/resolvers": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "next",
    "@remix-run/node": "latest",
    "@remix-run/react": "latest",
    "@remix-run/serve": "latest",
    "@vercel/remix": "latest",
    "graphql": "latest",
    "firebase": "latest",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "i18next-fetch-backend": "latest",
    "isbot": "latest",
    "pretty-cache-header": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "latest",
    "react-i18next": "latest",
    "remix-hook-form": "latest",
    "remix-i18next": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@jest/globals": "latest",
    "@remix-run/dev": "latest",
    "@testing-library/dom": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/jest": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "husky": "latest",
    "jest": "latest",
    "lint-staged": "latest",
    "msw": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-env-only": "latest",
    "vite-tsconfig-paths": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts, *.tsx": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
